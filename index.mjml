                text-transform="uppercase"
                align="center"
                font-size="32px"
                font-family="serif"
                letter-spacing="3px"
                padding-top="30px"
            />
            <mj-class
                name="caption"
                align="center"
                font-size="16px"
                padding="15px"
            />
