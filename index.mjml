<mjml>
    <mj-head>
        <mj-attributes>
            <mj-text padding="0" font-size="20px" />
            <mj-button
                font-size="19px"
                font-weight="600"
                text-transform="uppercase"
                align="center"
                color="#000"
                font-weight="800"
                background-color="transparent"
                padding="0"
                inner-padding="0"
            />
            <mj-class 
            <mj-class
                name="footer-btn"
                text-transform="none"
                font-weight="500"
            />
            <mj-social-element background-color="#fff" padding="0 20px" />
